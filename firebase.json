{
  "hosting": [
    {
      "target": "app",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "timeline-data",
      "public": "public" , 
      "rewrites": [
        {
          "source": "/timeline.json",
          "destination": "/timeline.json"
        }
      ]
    }
  ]
}